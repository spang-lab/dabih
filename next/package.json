{
  "name": "dabih-client",
  "version": "1.15.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "client": "cp -r ../api/build/api.ts ../api/build/schema.d.ts ../api/src/api/types ./lib/api/",
    "start": "npm run client && next dev",
    "test": "ava",
    "prod": "NODE_ENV=production next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@eslint/js": "^9.26.0",
    "@headlessui/react": "^2.2.2",
    "@tailwindcss/postcss": "^4.1.5",
    "ava": "^6.3.0",
    "jose": "^6.0.10",
    "jszip": "^3.10.1",
    "next": "15.3.1",
    "next-auth": "^5.0.0-beta.25",
    "openapi-fetch": "^0.13.7",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-feather": "^2.0.10",
    "react-to-print": "^3.1.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.1.2",
    "eslint": "^9.26.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.5"
  },
  "ava": {
    "nodeArguments": [
      "--import=tsx"
    ],
    "extensions": {
      "ts": "commonjs"
    },
    "files": [
      "./**/*.test.ts"
    ]
  },
  "overrides": {},
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "net": false,
    "tls": false
  }
}
