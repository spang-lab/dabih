import base64url from './base64url';
import CONST from './const';

const generate = async () => {
  const isExportable = true;
  const { privateKey, publicKey } = await crypto.subtle.generateKey(
    CONST.rsa,
    isExportable,
    ['encrypt', 'decrypt'],
  );
  return privateKey;
};

