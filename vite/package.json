{
  "name": "vite",
  "private": true,
  "version": "2.0.7",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "openapi": "cp -r ../api/build/api.ts ../api/build/schema.d.ts ../api/src/api/types ./src/lib/api/",
    "start": "npm run openapi && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "ava"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@eslint/js": "^9.31.0",
    "@headlessui/react": "^2.2.4",
    "jose": "^6.0.12",
    "jszip": "^3.10.1",
    "openapi-fetch": "^0.14.0",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-feather": "^2.0.10",
    "react-router": "^7.7.0",
    "react-router-dom": "^7.7.0",
    "react-to-print": "^3.1.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "ava": "^6.4.1",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.5",
    "vite-plugin-pages": "^0.33.1"
  },
  "ava": {
    "nodeArguments": [
      "--import=tsx"
    ],
    "extensions": {
      "ts": "commonjs"
    },
    "files": [
      "./**/*.test.ts"
    ]
  }
}
